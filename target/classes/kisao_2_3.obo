property_value: seeAlso http://identifiers.org/doi/10.1016/0771-050X(80)90013-3 xsd:anyURI {comment="Dormand J, Prince P. A family of embedded Runge-Kutta formulae. Journal of Computational and Applied Mathematics, Volume 6, pages 19-26 (1980)."}
property_value: seeAlso http://identifiers.org/pubmed/14990450 xsd:anyURI {comment="Takahashi K, Kaizu K, Hu B, Tomita M. A multi-algorithm, multi-timescale method for cell simulation. Bioinformatics volume 20(4), pages 538-46 (2004)."}

[Term]
id: KISAO:0000088
name: LSODA
is_a: KISAO:0000094 ! Livermore solver
disjoint_from: KISAO:0000089 ! LSODAR
relationship: KISAO:0000259 KISAO:0000209 {cardinality="1"} ! relative tolerance
relationship: KISAO:0000259 KISAO:0000211 {cardinality="1"} ! absolute tolerance
