property_value: isImplementedIn "JSim" xsd:string
property_value: seeAlso http://identifiers.org/pubmed/1449234 xsd:anyURI {comment="Bassingthwaighte JB, Chan IS, and Wang CY. Computationally efficient algorithms for capillary convection-permeation-diffusion models for blood-tissue exchange. Ann Biomed Eng 20: 687-725, 1992."}

